@model TicketPortal.Domain.Entities.AppUser

@{
    ViewData["Title"] = "User Details";
}

<h1>User Details</h1>

<div class="row">
    <div class="col-md-6">
        <dl>
            <dt>Username</dt>
            <dd>@Model.UserName</dd>
            <dt>Email</dt>
            <dd>@Model.Email</dd>
            <dt>Phone number</dt>
            <dd>@Model.PhoneNumber</dd>
            <dt>First name</dt>
            <dd>@Model.FirstName</dd>
            <dt>Last name</dt>
            <dd>@Model.LastName</dd>
        </dl>
    </div>
</div>
